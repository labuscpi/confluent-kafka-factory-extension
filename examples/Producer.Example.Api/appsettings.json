{
  "Kafka": {
    "Producers": {
      "Contradiction": {
        "Topic": "TO_BE_COMPLETED",
        "Config": {
          "Acks": "All",
          "BootstrapServers": "TO_BE_COMPLETED",
          "Debug": null,
          "SaslMechanism": "Plain",
          "SaslPassword": "TO_BE_COMPLETED",
          "SaslUsername": "$ConnectionString",
          "SecurityProtocol": "SaslSsl",
          "SocketKeepaliveEnable": true,
          "SocketTimeoutMs": 30000,
          "MessageTimeoutMs": 10000
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
